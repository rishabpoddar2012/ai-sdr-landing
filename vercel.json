{
  "version": 2,
  "name": "ai-sdr-landing",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/signup",
      "destination": "https://ai-sdr-app.vercel.app/signup"
    },
    {
      "source": "/login",
      "destination": "https://ai-sdr-app.vercel.app/login"
    }
  ]
}
